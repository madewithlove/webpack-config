{
  "name": "madewithlove-webpack-config",
  "version": "5.1.6",
  "description": "Configuration for Webpack projects at Madewithlove",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/madewithlove/webpack-config.git"
  },
  "keywords": [
    "webpack",
    "config"
  ],
  "author": {
    "name": "Madewithlove",
    "email": "heroes@madewithlove.be"
  },
  "license": "MIT",
  "dependencies": {
    "@webpack-blocks/babel6": "^0.3.0",
    "@webpack-blocks/webpack2": "^0.4.0-rc.2",
    "assets-webpack-plugin": "^3.5.1",
    "babel-preset-react-app": "^2.0.1",
    "babel-preset-stage-0": "^6.22.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.4",
    "clean-webpack-plugin": "^0.1.15",
    "merge": "^1.2.0"
  },
  "devDependencies": {
    "@webpack-blocks/sass": "^0.4.0",
    "babel-cli": "^6.22.2",
    "babel-preset-latest": "^6.22.0",
    "react": "^15.4.2",
    "webpack": "^2.0"
  },
  "peerDependencies": {
    "webpack": "*"
  },
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "yarn build",
    "postversion": "git push; git push --tags; npm publish",
    "lint": "eslint src",
    "test:acceptance": "cd tests/acceptance && webpack --bail",
    "test:acceptance:production": "NODE_ENV=production yarn test:acceptance",
    "test:unit": "jest",
    "test": "yarn test:acceptance && yarn test:acceptance:production && yarn test:unit"
  },
  "babel": {
    "presets": [
      "latest"
    ]
  },
  "eslintConfig": {
    "extends": "madewithlove",
    "rules": {
      "filenames/match-exported": 0
    }
  }
}
